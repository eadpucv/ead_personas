<div class="tab-pane fade active in" id="filtrar">
	<form>
		<h5 class="sans rojo">Creador de Lista de Correos e[ad]</h5>
		<div class="fila flexbox">
			<div class="col-md-4 container-flex">
				<label class="derecha flex-centrar" for="type">Tipo</label>
			</div>
			<div class="col-md-8 container-flex">
				<div class="checkbox bloque">
					<label>
						<input type="checkbox"> Alumno
					</label>
				</div>
				<div class="checkbox bloque">
					<label>
						<input type="checkbox"> Ex-Alumno
					</label>
				</div>
			</div>
		</div>
		<div class="fila flexbox">
			<div class="col-md-4 container-flex">
				<label class="derecha flex-centrar" for="course">Carrera</label>
			</div>
			<div class="col-md-8 container-flex">
				<div class="checkbox bloque">
					<label>
						<input type="checkbox"> Arquitectura
					</label>
				</div>
				<div class="checkbox bloque">
					<label>
						<input type="checkbox"> Diseño
					</label>
				</div>
			</div>
		</div>
		<div class="fila flexbox">
			<div class="col-md-4 container-flex">
				<label class="derecha flex-centrar" for="course">Año ingreso desde</label>
			</div>
			<div class="col-md-8 container-flex">
				<div class="grupo-inputs">
					<div class="input"><i class="icn icn-md icn-ingresar"></i></div>
					<input class="control" id="startdate" name="startdate" min="1972-01-01" max="2016-01-01" type="date">
				</div>
			</div>
		</div>
		<div class="fila flexbox">
			<div class="col-md-4 container-flex">
				<label class="derecha flex-centrar" for="course">Hasta</label>
			</div>
			<div class="col-md-8 container-flex">
				<div class="grupo-inputs">
					<div class="input"><i class="icn icn-md icn-salir"></i></div>
					<input class="control" id="enddate" name="enddate" min="1973-01-01" max="2016-01-01" type="date">
				</div>
			</div>
		</div>
		<div class="fila flexbox">
			<div class="col-md-4 container-flex">
				<label class="derecha flex-centrar" for="liststyle">Tipo de lista</label>
			</div>
			<div class="col-md-8 container-flex">
				<div class="checkbox">
					<label>
						<input type="radio" name="listtype"> Lista de correo
						<input type="radio" name="listtype"> Surveymonkey
					</label>
				</div>
			</div>
		</div>
		<div class="fila">
			<div class="col-md-8 col-md-offset-4">
				<button type="button" class="btn btn-accion btn-md btn-bloque btn-consultar">Consultar</button>
			</div>
		</div>
		<!-- Resultados -->
		<div class="fila bloque-resultado">
			<div class="col-md-12">
				<div class="margen-inf-sm">
					<div class="alerta alerta-accion alert-dismissible fade in" role="alert">
						<span><b>106</b> usuarios encontrados:
						</span>
					</div>
				</div>
			</div>
			<div class="col-md-12">
				<textarea name="emailList" id="emailList" rows="5" readonly="" onclick="this.focus();this.select()">
				</textarea>
			</div>
		</div>
